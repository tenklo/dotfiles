#!/bin/sh

brows=o
engines=~/.local/share/data/engines
menu=dmenu

engine=$(sort $engines | cut -d';' -f1 | $menu)
[ -z "$engine" ] && exit
url=$(grep "$engine" $engines | cut -d';' -f2)
[ -z "$url" ] && exit

test -n "$1" && search="$@" || search=$(echo "https://" | $menu )
test -z "$search" && exit

[ "$url" = "none" ] && echo "$search" || echo "$url$search"
